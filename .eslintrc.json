{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "eol-last": ["off"],
      "object-curly-newline": ["off"],
      "no-param-reassign": ["error", { "props": false }],
      "brace-style": ["error", "stroustrup"]
    }
}